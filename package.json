{
  "name": "jspm-ember",
  "version": "0.1.0",
  "description": "Demonstration of Ember 1.12-beta.1 and JSPM",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/OrKoN/jspm-ember-playground.git"
  },
  "devDependencies": {
    "browser-sync": "^1.6.3",
    "gulp": "^3.8.11",
    "jspm": "^0.15.3"
  },
  "scripts": {
    "postinstall": "jspm install"
  },
  "jspm": {
    "directories": {
      "baseURL": "app",
      "lib": "app"
    },
    "dependencies": {
      "ember": "github:components/ember@1.12.0-beta.1",
      "ember-computed-decorators": "github:rwjblue/ember-computed-decorators@master",
      "hbs": "github:n-fuse/plugin-ember-hbs@1.12.0-beta.1",
      "ihbs": "github:n-fuse/plugin-ember-ihbs@1.12.0-beta.1",
      "jquery": "github:components/jquery@^2.0.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.11",
      "babel-runtime": "npm:babel-runtime@^5.1.11",
      "core-js": "npm:core-js@^0.8.1"
    },
    "overrides": {
      "github:components/ember@1.12.0-beta.1": {
        "main": "ember.debug",
        "files": [
          "ember.debug.js"
        ],
        "dependencies": {
          "jquery": "github:components/jquery@^2.0.3"
        },
        "shim": {
          "ember.debug": {
            "deps": [
              "jquery"
            ],
            "exports": "Ember"
          }
        }
      },
      "github:rwjblue/ember-computed-decorators@master": {
        "main": "addon/index.js",
        "files": [
          "addon/index.js"
        ],
        "dependencies": {
          "ember": "jspm:ember@*",
          "ember-new-computed": "github:rwjblue/ember-new-computed@master"
        }
      },
      "github:rwjblue/ember-new-computed@master": {
        "main": "addon/index.js",
        "files": [
          "addon/index.js",
          "addon/utils/can-use-new-syntax.js"
        ],
        "dependencies": {
          "ember": "jspm:ember@*"
        }
      }
    }
  },
  "dependencies": {
    "jspm": "git://github.com/jspm/jspm-cli#0.15.4"
  }
}
