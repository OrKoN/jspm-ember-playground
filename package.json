{
  "name": "jspm-ember",
  "version": "0.1.0",
  "description": "Demonstration of Ember 1.12-beta.1 and JSPM",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/OrKoN/jspm-ember-playground.git"
  },
  "scripts": {
    "postinstall": "jspm install"
  },
  "devDependencies": {
    "browser-sync": "^1.6.3",
    "gulp": "^3.8.11",
    "jspm": "^0.15.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "app",
      "lib": "app"
    },
    "dependencies": {
      "ember": "github:components/ember@1.12.0-beta.1",
      "hbs": "github:n-fuse/plugin-ember-hbs@1.12.0-beta.1",
      "ihbs": "github:n-fuse/plugin-ember-ihbs@1.12.0-beta.1",
      "jquery": "github:components/jquery@^2.0.3",
      "ember-vcl-calendar": "github:ember-vcl/calendar@^0.1.0"
    },
    "devDependencies": {
      "babel": "npm:babel@^4.7.16",
      "babel-runtime": "npm:babel-runtime@^4.7.16"
    },
    "overrides": {
      "github:components/ember@1.12.0-beta.1": {
        "main": "ember.debug",
        "files": [
          "ember.debug.js"
        ],
        "dependencies": {
          "jquery": "github:components/jquery@^2.0.3"
        },
        "shim": {
          "ember.debug": {
            "deps": [
              "jquery"
            ],
            "exports": "Ember"
          }
        }
      }
    }
  }
}
